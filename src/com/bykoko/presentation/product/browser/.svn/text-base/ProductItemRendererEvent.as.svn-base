package com.bykoko.presentation.product.browser
{
	import com.bykoko.vo.xmlmapping.product.Articulo;
	
	import flash.events.Event;
	
	public class ProductItemRendererEvent extends Event
	{
		public static const SELECT:String = "select";
		public var articulo:Articulo;
		
		public function ProductItemRendererEvent(type:String, articulo:Articulo)
		{
			super(type);
			this.articulo = articulo;
		}
	}
}